  "scripts": {
    "test:all": "yarn test:node",
    "build": "rm -rf esnext && tsc",
    "build:watch": "tsc-watch --onSuccess \"yarn postbuild\"",
    "prebuild": "node scripts/pre-build.js",
    "postbuild": "mkdir -p coverage && yarn tailwind && node scripts/post-build.js",
    "tailwind": "tailwindcss -i src/karma/react/server/server.css -o esnext/karma/react/server/server.css",
    "posttest": "mkdir -p coverage && node scripts/post-test.js",
    "postcoverage": "mkdir -p coverage && node scripts/post-test.js",
    "generate": "yarn build && node esnext/generate.js",
    "test": "yarn build && yarn test:node",
    "test:node": "export $(cat .env | xargs) && OTEL_SERVICE_NAME=karma-tests node --enable-source-maps esnext/tests/index.js",
    "test:node:inspect": "yarn build && export $(cat .env | xargs) && OTEL_SERVICE_NAME=karma-tests node --enable-source-maps --inspect-brk esnext/tests/index.js",
    "coverage": "export $(cat .env | xargs) && OTEL_SERVICE_NAME=karma-tests c8 node esnext/tests/index.js && yarn postbuild",
    "start": "node --enable-source-maps esnext/karma/listen/main.js",
    "start:nodemon": "nodemon --enable-source-maps esnext/karma/listen/main.js",
    "quick": "yarn build && yarn start",
    "local": "yarn build && yarn seed && yarn start",
    "seed": "node --enable-source-maps esnext/karma/data/seed/main.js",
    "watch": "yarn build && concurrently --raw \"yarn start:nodemon\" \"yarn build:watch\" ",
    "prettier:check": "prettier --check .",
    "prettier": "prettier --write ."
  }
